┌───────────────────────────────────| Aliases |───────────────────────────────────────────────┐
                   **Copie e cole na pasta /home/nome/.bashrc**

  alias matrix='cmatrix'
  alias neofetch='neofetch | lolcat'
  alias hosts='sudo nano /etc/hosts'
  alias lsd='ls -d */'
  alias lsn='ls -d'
  alias bashrc='sudo nano /home/nicolas/.bashrc'
  alias help='more /home/nicolas/.help'
  alias ascii='more /home/nicolas/.ascii.txt'
  alias cachels="sudo ls /var/cache/pacman/pkg/ | wc -l"
  alias cachesize="du -sh /var/cache/pacman/pkg/"
  alias helpgit="more /home/nicolas/./.helpgit"
  alias manjaro="cat /etc/lsb-release"
  alias lswifi="hwinfo --netcard --wlan --bluetooth | grep -Ei 'model\:|driver\:|status\:|cmd\:|file\:|detected\:' | grep -v 'Config Status'"
  

